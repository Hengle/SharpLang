.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  SharpLang.Runtime_config = debug
endif
ifeq ($(config),release)
  SharpLang.Runtime_config = release
endif

PROJECTS := SharpLang.Runtime

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

SharpLang.Runtime:
ifneq (,$(SharpLang.Runtime_config))
	@echo "==== Building SharpLang.Runtime ($(SharpLang.Runtime_config)) ===="
	@${MAKE} --no-print-directory -C . -f SharpLang.Runtime.make config=$(SharpLang.Runtime_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f SharpLang.Runtime.make clean

